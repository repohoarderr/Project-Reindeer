{
  "name": "project-reindeer-frontend",
  "version": "0.1.0",
  "description": "Basic frontend starting point for building the MDC pricing page.",
  "main": "client/index.jsx",
  "type": "module",
  "scripts": {
    "client": "esbuild client/index.jsx --target=es6 --bundle --minify --outdir=public",
